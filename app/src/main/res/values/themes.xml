    <style name="Theme.blue.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.blue.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


    <style name="Theme.dark.NoActionBar">
        <item name="windowActionBar">false</item>
